<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>professorBean</managed-bean-name>
		<managed-bean-class>br.com.cadei.bean.ProfessorBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>alunoBean</managed-bean-name>
		<managed-bean-class>br.com.cadei.bean.AlunoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>registroBean</managed-bean-name>
		<managed-bean-class>br.com.cadei.bean.RegistrosBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>criarTabelas</managed-bean-name>
		<managed-bean-class>br.com.cadei.config.CriarTabelas</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>br.com.cadei.bean.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>folhaBean</managed-bean-name>
		<managed-bean-class>br.com.cadei.bean.FolhaBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>graficoBean</managed-bean-name>
		<managed-bean-class>br.com.cadei.grafico.ChartView</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

</faces-config>
